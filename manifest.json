{
  "name": "Kadenze Forum Modernizer",
  "manifest_version" : 2,
  "version": "1.2.8",
  "description": "",
  "page_action": {
    "default_icon": "calendar_icon_16.png"
  },
  "icons": {
    "16" : "calendar_icon_16.png",
    "48" : "calendar_icon_48.png",
    "128" : "calendar_icon_128.png"
  },
  "permissions": [
    "http://*.kadenze.com/*",
    "https://*.kadenze.com/*",
    "http://*.google.com/*",
    "https://*.google.com/*"
  ],
  "content_scripts": [
    {
      "matches" : ["*://www.kadenze.com/*"],
      "js" : ["jquery.js", "facebook_event.js"],
      "css" : ["facebook_event.css"]
    }
  ]
}
